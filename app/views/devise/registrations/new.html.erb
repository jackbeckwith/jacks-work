<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <div class="form">
    <h2>Sign up</h2>
    <div class="field"><%= f.label :name %>
  	 <%= f.text_field :name %></div>

    <div class="field"><%= f.label :email %>
    <%= f.email_field :email, :autofocus => true %>

    <div class="field"><%= f.label :hometown %>
    <%= f.text_field :hometown %></div>

    <div class="field">Music Style
    <%= f.text_field :style %></div>

    <div class="field"><%= f.label :password %>
    <%= f.password_field :password %></div>

    <div class="field"><%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %></div>

    <div class="button3"><%= f.submit "Sign up" %></div>
    <%= devise_error_messages! %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
